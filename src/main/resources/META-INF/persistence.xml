<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="3.1"
	xsi:schemalocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_1.xsd">
	<persistence-unit name="default"
		transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<exclude-unlisted-classes>false</exclude-unlisted-classes>
		<properties>
		
			<!--<property name="javax.persistence.nonJtaDataSource" value="java:comp/env/jdbc/packlist-datasource"/>-->
		</properties>
		<non-jta-data-source>java:comp/env/jdbc/packlist-datasource</non-jta-data-source>
	</persistence-unit>
</persistence>